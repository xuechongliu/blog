<?xml version="1.0" encoding="UTF-8" ?>

<!-- see docs at http://code.google.com/intl/en/apis/customsearch/docs/context.html -->

<GoogleCustomizations>
  <CustomSearchEngine>
    <Title><%= t('shared.Airbnb FAQs', :default => "Airbnb FAQs")%></Title>
    <Description></Description>
    <Context>
      <BackgroundLabels>
        <Label name="airbnb" mode="FILTER" />
      </BackgroundLabels>
    </Context>
    <LookAndFeel>
      <Colors url="#FF6600" background="#FFFF33" border="#FF6600" title="#FF3300" text="#000000" visited="#663399" light="#FF0077"/>
    </LookAndFeel>
  </CustomSearchEngine>
  
  <!-- Add urls patterns to include in search results -->
  <Annotations>
    <Annotation about="http://*.airbnb.com/help/question/*">
      <Label name="airbnb"/>
    </Annotation>
  </Annotations>

</GoogleCustomizations>
